{
  "expo": {
    "name": "rn-self-posts",
    "slug": "rn-self-posts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "listatatStas.host.exp.expo",
      "buildNumber": "1.0.0"
    },
    "android": {
    
        "package": "com.listatatStas.self_blog_rn_app",
        "versionCode": 1,
        "permissions": [
          "CAMERA",
          "WRITE_EXTERNAL_STORAGE"
        ]
      }

    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }

